{
  "presets": [
    ["@babel/preset-env", {
      "targets": "cover 99.5% in US, IE 10-11"
    }],
    "@babel/react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime",
    {
      "corejs": false,
      "helpers": true,
      "regenerator": true,
      "useESModules": true
    }],
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
